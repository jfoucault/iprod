        <div class="deletion_main_container">
            {% for data in data %}
                   <div class="objectDLTwrapper">
                       <a href="{{ data['Place'] }}" >{{ data['Place'] }}</a>
                   </div>
            {% endfor %}
        </div>
    {% endblock %}
